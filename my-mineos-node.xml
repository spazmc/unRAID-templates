<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>mineos-node</Name>
  <Description>Automated build of mineos-node.&#13;
[b]Converted By Community Applications[/b]</Description>
  <Registry>https://hub.docker.com/r/jgfield/mineos-node/~/dockerfile/</Registry>
  <Repository>jgfield/mineos-node</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8443</HostPort>
        <ContainerPort>8443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>25566</HostPort>
        <ContainerPort>25566</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>25566</HostPort>
        <ContainerPort>25566</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>25567</HostPort>
        <ContainerPort>25567</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>25567</HostPort>
        <ContainerPort>25567</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>25565</HostPort>
        <ContainerPort>25565</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>25565</HostPort>
        <ContainerPort>25565</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/apps/mcnode</HostDir>
      <ContainerDir>/var/games/minecraft</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>https://[IP]:[PORT:8443]</WebUI>
  <Banner>http://icons.iconarchive.com/icons/chrisl21/minecraft/48/Creeper-icon.png</Banner>
  <Icon>http://icons.iconarchive.com/icons/chrisl21/minecraft/48/Creeper-icon.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
